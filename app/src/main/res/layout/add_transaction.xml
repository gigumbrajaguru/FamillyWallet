<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ccpe001.familywallet.transaction.add_transaction">

    <AbsoluteLayout
        android:layout_width="418dp"
        android:layout_height="514dp"
        android:minHeight="179dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333"
        tools:layout_editor_absoluteX="2dp">

        <Spinner
            android:id="@+id/spinner7"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:layout_weight="1"
            android:layout_x="251dp"
            android:layout_y="24dp"
            android:entries="@array/spinnerCurrency"
            tools:layout_editor_absoluteX="246dp"
            tools:layout_editor_absoluteY="37dp" />

        <EditText
            android:id="@+id/editText9"
            android:layout_width="186dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_x="64dp"
            android:layout_y="24dp"
            android:ems="10"
            android:gravity="right"
            android:inputType="numberDecimal"
            android:text="1500.00"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="37dp" />

        <RadioButton
            android:id="@+id/radioButton5"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_x="217dp"
            android:layout_y="90dp"
            android:checked="false"
            android:text="Income"
            tools:layout_editor_absoluteX="197dp"
            tools:layout_editor_absoluteY="117dp" />

        <EditText
            android:id="@+id/editText10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_x="88dp"
            android:layout_y="157dp"
            android:ems="10"
            android:hint="Title"
            android:inputType="textPersonName"
            android:textAlignment="center"
            tools:layout_editor_absoluteX="85dp"
            tools:layout_editor_absoluteY="185dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="246dp"
            android:layout_height="31dp"
            android:layout_weight="1"
            android:layout_x="92dp"
            android:layout_y="286dp"
            android:text="Category"
            tools:layout_editor_absoluteX="103dp"
            tools:layout_editor_absoluteY="333dp" />

        <Spinner
            android:id="@+id/spinner8"
            android:layout_width="268dp"
            android:layout_height="38dp"
            android:layout_weight="1"
            android:layout_x="78dp"
            android:layout_y="228dp"
            android:entries="@array/spinnerAccount"
            android:gravity="center"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="258dp" />

        <RadioButton
            android:id="@+id/radioButton6"
            android:layout_width="122dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_x="80dp"
            android:layout_y="90dp"
            android:checked="true"
            android:text="Expense"
            tools:layout_editor_absoluteX="58dp"
            tools:layout_editor_absoluteY="117dp" />


        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="292dp"
            android:layout_y="747dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/txtLocation"
            android:layout_width="244dp"
            android:layout_height="36dp"
            android:layout_x="95dp"
            android:layout_y="414dp"
            android:text="TextView" />

        <EditText
            android:id="@+id/txtTime"
            android:layout_width="wrap_content"
            android:layout_height="51dp"
            android:layout_x="241dp"
            android:layout_y="350dp"
            android:editable="false"
            android:text="Set Time"
            android:textAlignment="center" />

        <EditText
            android:id="@+id/txtDate"
            android:layout_width="112dp"
            android:layout_height="56dp"
            android:layout_x="100dp"
            android:layout_y="347dp"
            android:editable="false"
            android:hint="Set Date"
            android:textAlignment="center" />

        <Button
            android:id="@+id/button6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="103dp"
            android:layout_y="452dp"
            android:text="Button" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="59dp"
            android:layout_height="65dp"
            android:layout_x="6dp"
            android:layout_y="16dp"
            app:srcCompat="@mipmap/price" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="14dp"
            android:layout_y="85dp"
            app:srcCompat="@mipmap/income_expense" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="15dp"
            android:layout_y="151dp"
            app:srcCompat="@mipmap/note1" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="67dp"
            android:layout_height="wrap_content"
            android:layout_x="5dp"
            android:layout_y="219dp"
            app:srcCompat="@mipmap/account" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="35dp"
            android:layout_height="38dp"
            android:layout_x="21dp"
            android:layout_y="350dp"
            app:srcCompat="@mipmap/calander" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="14dp"
            android:layout_y="290dp"
            app:srcCompat="@mipmap/category" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="16dp"
            android:layout_y="408dp"
            app:srcCompat="@mipmap/location" />

    </AbsoluteLayout>

</android.support.constraint.ConstraintLayout>
